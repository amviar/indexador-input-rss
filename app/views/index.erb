<div id='existing-feeds' class="row">
  <%= erb :feeds_table, locals: {feeds: @feeds} %>
</div>
<div class='row'>
  <h2>Nuevo Feed</h2>
  <form action='/feeds' method='post' >
  <div class="form-group">
    <label for='feed[name]'>Nombre</label>
    <input type='text' name='feed[name]' class="form-control">
  </div>
  <div class="form-group">
    <label for='feed[url]'>URL</label>
    <input type='text' name='feed[url]' class="form-control" />
  </div>

  <input type='submit' value='Crear' class="btn btn-primary" />
  </form>
</div>
